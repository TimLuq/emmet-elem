{
  "name": "emmet-elem",
  "version": "0.1.3",
  "description": "Lightweight emmet expansion to DOM",
  "main": "emmet-elem.js",
  "module": "emmet-elem.js",
  "browser": "umd/emmet-elem.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./emmet-elem.ts",
      "node": {
        "import": "./emmet-elem.js",
        "default": "./cjs/emmet-elem.cjs"
      },
      "default": {
        "import": "./emmet-elem.js",
        "default": "./umd/emmet-elem.js"
      }
    },
    "./emmet-elem.js": {
      "types": "./emmet-elem.ts",
      "node": {
        "import": "./emmet-elem.js",
        "default": "./cjs/emmet-elem.cjs"
      },
      "default": {
        "import": "./emmet-elem.js",
        "default": "./umd/emmet-elem.js"
      }
    },
    "./emmet-compress.js": {
      "types": "./emmet-compress.ts",
      "node": {
        "import": "./emmet-compress.js",
        "default": "./cjs/emmet-compress.cjs"
      },
      "default": {
        "import": "./emmet-compress.js",
        "default": "./umd/emmet-compress.js"
      }
    },
    "./emmet-loader.js": {
      "types": "./emmet-loader.ts",
      "node": {
        "import": "./emmet-loader.js",
        "default": "./cjs/emmet-loader.cjs"
      },
      "default": {
        "import": "./emmet-loader.js",
        "default": "./umd/emmet-loader.js"
      }
    },
    "./emmet-slot.js": {
      "types": "./emmet-slot.ts",
      "node": {
        "import": "./emmet-slot.js",
        "default": "./cjs/emmet-slot.cjs"
      },
      "default": {
        "import": "./emmet-slot.js",
        "default": "./umd/emmet-slot.js"
      }
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "node ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimLuq/emmet-elem.git"
  },
  "keywords": [
    "emmet",
    "dom",
    "web"
  ],
  "author": "TimLuq",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/TimLuq/emmet-elem/issues"
  },
  "homepage": "https://github.com/TimLuq/emmet-elem#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "fallback-dom": "github:timluq/fallback-dom",
    "htmlparser2": "^8.0.2",
    "rollup": "^3.20.2",
    "rollup-plugin-summary": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "htmlparser2": "^8.0.2"
  }
}
